set spoolfile      = +INBOX
mailboxes          = +INBOX
unset imap_passive
set imap_keepalive = 300
set mail_check     = 120
set header_cache   = ~/.cache/mutt

#SMTP
unset record
set smtp_url=smtps://$imap_user:$imap_pass@smtp.gmail.com

alternative_order text/plain text/html

